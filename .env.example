# Velib GBFS endpoints
VELIB_GBFS_URL=https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole/gbfs.json
VELIB_STATION_STATUS_URL=https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole/station_status.json
VELIB_STATION_INFORMATION_URL=https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole/station_information.json
VELIB_SYSTEM_INFORMATION_URL=https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole/system_information.json

# HTTP behavior
VELIB_TIMEOUT_SECONDS=10
VELIB_RETRY_ATTEMPTS=3
VELIB_RETRY_BACKOFF_SECONDS=1
VELIB_POLL_INTERVAL_SECONDS=60
VELIB_LOG_LEVEL=INFO
VELIB_USER_AGENT=velib-ingestion/0.1.0

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC_STATUS=velib.station_status
KAFKA_TOPIC_INFORMATION=velib.station_information
KAFKA_CLIENT_ID=velib-ingestion
KAFKA_GROUP_ID=velib-consumer
KAFKA_ENABLE_IDEMPOTENCE=true
